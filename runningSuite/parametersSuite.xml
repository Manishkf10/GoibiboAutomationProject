		<classes>
			<class name="com.goibibo.TestClass.TC001LogoTest" />
			<!--class name="com.goibibo.TestClass.TC002FlightBookingTest" /-->
		</classes>
	</test> <!--
	Test -->
</suite> <!--
ParallelSuite -->	