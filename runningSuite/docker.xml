<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelSuite" thread-count="1" parallel="tests">


	<test name="firefoxTest">
		<parameter name="executionEnvironment" value="remote" />
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.goibibo.TestClass.TC001LogoTest" />
		</classes>
	</test>


	<test name="chromeTest">
		<parameter name="executionEnvironment" value="remote" />
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.goibibo.TestClass.TC001LogoTest" />
		</classes>
	</test>


	<test name="edgeTest">
		<parameter name="executionEnvironment" value="remote" />
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.goibibo.TestClass.TC001LogoTest" />
		</classes>
	</test>


</suite> <!--
ParallelSuite -->	